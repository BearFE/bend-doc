@font-face
    font-family  "bicon";
    src url('../public/assets/fonts/bfont.eot'); /* IE9*/
    src url('../public/assets/fonts/bfont.eot') format('embedded-opentype'), /* IE6-IE8 */
    url('data:application/x-font-woff;charset=utf-8;base64,') format('font-woff'),
    url('../public/assets/fonts/bfont.ttf') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('../public/assets/fonts/bfont.svg') format('svg'); /* iOS 4.1- */

